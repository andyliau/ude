<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Compiled "C# port of Mozilla Universal Charset Detector" for .NET Framework, .NET Core </Description>
    <Authors>https://github.com/chrisquirk/ude</Authors>
    <TargetFrameworks>net452;net46;netstandard1.3;netstandard2.0</TargetFrameworks>
    <AssemblyName>Ude</AssemblyName>
    <RootNamespace>Ude</RootNamespace>
    <PackageId>Ude.NET</PackageId>
    <PackageProjectUrl>https://github.com/chrisquirk/ude</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/chrisquirk/ude/blob/master/license/MPL-1.1.txt</PackageLicenseUrl>
    <PackageTags>UDE;Encoding detection;mozilla universal charset detector; charset detection; character set detection</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
</Project>
